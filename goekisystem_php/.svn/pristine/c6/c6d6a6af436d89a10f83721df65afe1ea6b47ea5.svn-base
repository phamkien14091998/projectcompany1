@media screen and (max-width: 991px){.flex-wrap{-js-display:flex;display:-webkit-box;display:-moz-box;display:-ms-flexbox;display:-webkit-flex;display:flex;overflow-x:scroll}.flex-wrap>div{-webkit-flex-shrink:0;-moz-flex-shrink:0;-ms-flex-shrink:0;flex-shrink:0;float:none}}.girls-box{cursor:pointer;display:block;margin-bottom:10px;overflow:hidden;padding:15% 0;position:relative;text-align:center;transition:.3s ease;transform:perspective(0)}.girls-box:hover{box-shadow:0 4px 20px 0 rgba(0,0,0,0.14),0 7px 10px -5px rgba(3,166,120,0.4)}.girls-box:hover img{transform:scale(1.05) perspective(0)}.girls-box .girls-head{position:relative;z-index:0}.girls-box .girls-img{border-radius:50%;overflow:hidden;height:0;margin:auto;padding-top:90%;position:relative;width:90%;z-index:0}.girls-box .girls-img>img{bottom:0;left:0;position:absolute;right:0;top:0;transition:.3s ease}.girls-box .girls-icon{border-radius:1in;background:rgba(239,130,182,0.8);color:white;display:block;height:44px;left:4%;padding:13px 0 0;position:absolute;text-align:center;top:-4%;width:44px;z-index:1}@media screen and (min-width: 1500px){.girls-box .girls-icon{font-size:16px;height:64px;width:64px;padding:22px 0 0}}.girls-box .girls-name{color:#04d89c;font-size:145%;overflow:hidden;padding:7.5% 0;text-overflow:ellipsis}.girls-box .girls-name>.name{display:inline-block}.girls-box .schedule-time{background:rgba(3,166,120,0.95);bottom:0;color:#fff;font-size:140%;left:0;padding:5% 0;position:absolute;width:100%;z-index:1}.girls-box .girls-name,.girls-box .schedule-time{line-height:1;white-space:nowrap}.girls-box .girls-comment{color:#111}
